  "version": "1.0.0",
  "description": "墨滴插件",
  "icons": {
    "16": "目录.png",
    "48": "目录.png",
    "128": "目录.png"
  },
  "action": {
    "default_title": "墨滴插件",
    "default_icon": "目录.png",
